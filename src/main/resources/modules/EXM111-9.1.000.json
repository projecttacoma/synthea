        ],
        "location": [
          {
            "location": {
              "reference": "Location/REPLACE-UUID-0"
            }
          }
        ]
      }
    },
    "ED Visit End": {
      "type": "EncounterEnd",
      "direct_transition": "Encounter Inpatient"
    },
    "Encounter Inpatient": {
      "type": "Encounter",
      "direct_transition": "Hospitalization Duration",
      "codes": [
        {
          "code": 183452005,
          "system": "",
          "display": "Emergency hospital admission (procedure)",
          "value_set": "http://cts.nlm.nih.gov/fhir/ValueSet/2.16.840.1.113883.3.666.5.307"
        }
      ],
      "encounter_class": "inpatient"
    },
    "Time until ED Visit End": {
      "type": "Delay",
      "direct_transition": "ED Visit End",
      "range": {
        "low": 1,
        "high": 300,
        "unit": "minutes"
      }
    },
    "Hospitalization Duration": {
      "type": "Delay",
      "direct_transition": "Inpatient End",
      "range": {
        "low": 1,
        "high": 120,
        "unit": "days"
      }
    },
    "Inpatient End": {
      "type": "EncounterEnd",
      "distributed_transition": [
        {
          "transition": "Terminal",
          "distribution": 0.5
        },
        {
          "transition": "Initial",
          "distribution": 0.5
        }
      ]
    },
    "Measurement_Period": {
      "type": "Guard",
      "allow": {
        "condition_type": "And",
        "conditions": [
          {
            "condition_type": "Date",
            "operator": "==",
            "year": 2019,
            "value": 0
          },
          {
            "condition_type": "Date",
            "operator": "<",
            "month": 9,
            "value": 0
          }
        ]
      },
      "distributed_transition": [
        {
          "transition": "Diagnosis: Psychiatric/Mental Health Diagnosis",
          "distribution": 0.5
        },
        {
          "transition": "Pre-ED Visit",
          "distribution": 0.5
        }
      ]
    },
    "Diagnosis: Psychiatric/Mental Health Diagnosis": {
      "type": "ConditionOnset",
      "assign_to_attribute": "",
      "target_encounter": "Encounter Inpatient",
      "codes": [
        {
          "system": "SNOMED-CT",
          "code": 109006,
          "display": "Anxiety disorder of childhood OR adolescence (disorder)",
          "value_set": "http://cts.nlm.nih.gov/fhir/ValueSet/2.16.840.1.113883.3.117.1.7.1.299"
        }
